#. type: SS
#: data/man/svlang.1:52
#, no-wrap
msgid "consistency (alias: con)"
msgstr ""

#. type: Plain text
#: data/man/svlang.1:56
msgid ""
"Check translation consistency across PO or TS files. Finds cases where the "
"same source string has been translated differently, or where different "
"source strings share the same translation."
msgstr ""

#. type: TP
#: data/man/svlang.1:56
#, no-wrap
msgid "I<FILES>..."
msgstr ""

#. type: Plain text
#: data/man/svlang.1:59
msgid "One or more .po or .ts files to check."
msgstr ""

#. type: TP
#: data/man/svlang.1:59
#, no-wrap
msgid "B<-i>, B<--ignore-case>"
msgstr "B<-i>, B<--ignore-case>"

#. type: Plain text
#: data/man/svlang.1:62
msgid "Ignore case when comparing translations."
msgstr ""

#. type: Plain text
#: data/man/svlang.1:67
#, no-wrap
msgid "svlang consistency po/sv.po po/sv_extra.po\n"
msgstr ""

#. type: SS
#: data/man/svlang.1:69
#, no-wrap
msgid "compound (alias: split)"
msgstr ""

#. type: Plain text
#: data/man/svlang.1:72
msgid ""
"Split Swedish compound words into their components. Uses dictionary-based "
"decomposition to identify word parts."
msgstr ""

#. type: TP
#: data/man/svlang.1:72
#, no-wrap
msgid "I<WORDS>..."
msgstr ""

#. type: Plain text
#: data/man/svlang.1:75
msgid "One or more Swedish words to decompose."
msgstr ""

